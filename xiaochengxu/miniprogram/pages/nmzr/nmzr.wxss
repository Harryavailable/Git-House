.load image{
  width: 100%;
  height: 550rpx;
}
button{
background-color:rgb(157, 190, 179);
margin-top:30rpx;
font-size: 18px;
font-weight: 600;
width: 40%;
color:#f0f0f0;
}
.bigbox{  
  position:relative;
  width: 100vw;
  height:30vh;
  display: flex;
  flex-direction: column;
  justify-content:space-around;
  align-items:center;
}
.card{
  color: #333;
  position: absolute;
  width: 90vw;
  height:400rpx;
  border-radius: 20rpx;
  padding: 10rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  background: linear-gradient(to right bottom,
  #a1ffce79,
  #a1ffce6b,
  #faffd16b,
  #faffd14f
  
  );
  backdrop-filter: blue(8px);
  box-shadow: 10px 10px 10px rgba(0,0,0,0.2);
  border-top: 1px solid rgba(255,255,255,0.8);
  border-left: 1px solid rgba(255,255,255,0.8);
}
li{
display:flex;
height: 50rpx;
margin-bottom:10rpx;
justify-content:flex-start;
}
 .txt1{
  display: block;
  font-size: 15px;
  font-weight: 600;
}
.txt11{
  margin-right: 100rpx;
}
.txt12{
  margin-right: 123rpx;
}
li .txt2{
  font-weight: 400;
}